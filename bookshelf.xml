<?xml version="1.0"?>
<Container version="2">
  <Name>bookshelf</Name>
  <Repository>ghcr.io/pennydreadful/bookshelf:hardcover</Repository>
  <Registry>https://github.com/pennydreadful/bookshelf</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <ReadMe/>
  <Overview>Fork of Readarr</Overview>
  <Category/>
  <WebUI>http://[IP]:[PORT:8787]/</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/pennydreadful/bookshelf/blob/develop/Logo/128.png?raw=true</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1769021655</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires>tag must be changed from hardcover to softcover if coming from readarr to use goodreads metadata.</Requires>
  <Config Name="Webui" Target="8787" Default="8787" Mode="tcp" Description="Port to bookshelf web ui." Type="Port" Display="always" Required="true" Mask="false">8787</Config>
  <Config Name="config" Target="/config" Default="/mnt/user/appdata/bookshelf/" Mode="rw" Description="Bookshelf config file." Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/bookshelf/</Config>
  <Config Name="Downloads" Target="/downloads" Default="" Mode="rw" Description="Location of download client folder." Type="Path" Display="always" Required="false" Mask="false">/mnt/cache/Downloads/completed/</Config>
  <Config Name="media" Target="/media" Default="" Mode="rw" Description="Location of audiobooks on disk." Type="Path" Display="always" Required="false" Mask="false">/mnt/user/audiobooks/</Config>
  <TailscaleStateDir/>
</Container>
